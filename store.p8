pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init()
	i=0
	
	spawn_x = 4
	spawn_y = 1
	
	hero_x=spawn_x
	hero_y=spawn_y
end

function _update()
	if btn(0) then
	--right
		hero_x -= 8
	end
	if btn(1) then
	--left
		hero_x += 8
	end
	if btn(2) then
	--up
		hero_y -= 8
	end
	if btn(3) then
	--down
		hero_y += 8
	end
end

function _draw()
	cls()
	map(3,0,0,0,7,14)
	draw_hero()
end

function draw_hero()
	if i<3 then
		spr(1, hero_x, hero_y)
		i+=1
	else
		spr(2,hero_x, hero_y)
		i+=1
	end
	if i==6 then
		i=0
	end
end
