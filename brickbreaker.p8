pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init()
	circ_x = 127
	circ_y = 64
	circ_dx = -2
	circ_dy = 2
	circ_rad = 2
	
	pad_x0 = 49
	pad_y0 = 120
	pad_x1 = 79
	pad_y1 = 124
	pad_dx = 0
	pad_col = 15
	
	brick_w = 10
	brick_h = 4
	brick_c = 14
	num_bricks = 7
	
	brick_x = 2
	brick_y = 5
	brick_v = {}
	for i=1,num_bricks do
		add(brick_v, true)
	end
	
	points = 0
	
	side = 1
	surfaces = {vert=1, horiz=2}
	debug1 = ""
end

function _update()
	
	-- take input
	if btn(0) then
		pad_dx = -5
		if pad_x0 < 0 then
			pad_dx = 0
		end
	elseif btn(1) then
		pad_dx = 5
		if pad_x1 > 127 then
			pad_dx = 0
		end
	else
		if pad_x1 > 127 or pad_x0 < 0 then
			pad_dx = 0
		end
		pad_dx = pad_dx / 1.3
	end
	
	pad_x0 += pad_dx
	pad_x1 += pad_dx
	
	-- check for collision w/ walls
	if circ_x>128 then
		circ_dx = -3
		sfx(1)
	end
	if circ_x<0 then
		circ_dx = 3
		sfx(1)
	end
	if circ_y>128 then
		circ_dy = -3
		sfx(1)
	end
	if circ_y<0 then
		circ_dy = 3
		sfx(1)
	end
	
	
	-- check for collision w/ paddle
	detect_hit(pad_x0, pad_y0, pad_x1, pad_y1)
	if circ_box(pad_x0, pad_y0, pad_x1, pad_y1)	then
		sfx(2)
		if side == "horiz" then
			circ_dy *= -1
		else
			circ_dx *= -1
		end
		pad_col = 8
	else
		pad_col = 15
	end
	
	-- check for collision w/ brick_0
	for i=1,num_bricks do
		detect_hit((brick_x+brick_w)*i, 
				brick_y, 
				(((brick_x+brick_w)*i)+brick_w), 
				((brick_y)+brick_h))
		if brick_v[i] and circ_box((brick_x+brick_w)*i, 
				brick_y, 
				(((brick_x+brick_w)*i)+brick_w), 
				((brick_y)+brick_h))	then
			sfx(0)
			brick_v[i] = false
			points += 10
			if side == "horiz" then
				circ_dy *= -1
			else
				circ_dx *= -1
			end
		end
	end
	
	circ_x += circ_dx
	circ_y += circ_dy	
end 

function _draw()
	draw_score(points)
	--background
	rectfill(0,0,127,127,0)
	--ball
	circfill(circ_x, circ_y, circ_rad, 6)
	--paddle
	rectfill(pad_x0, pad_y0, pad_x1, pad_y1, pad_col)
	--bricks
	draw_bricks()
end

function circ_box(pad_x0, pad_y0, pad_x1, pad_y1)
	if circ_y + circ_rad < pad_y0 then
		return false
	end
	if circ_y + circ_rad > pad_y1 then
		return false
	end
	if (circ_x + circ_rad > pad_x1) then
		return false
	end
	if (circ_x + circ_rad < pad_x0)  then
		return false
	end
	return true
end

function detect_hit(pad_x0, pad_y0, pad_x1, pad_y1)
	local slp = circ_dy / circ_dx
	if slp > 0 and circ_dx >= 0 then
		-- moving down right
		tx = pad_x0-circ_x
		ty = pad_y0-circ_y
		if tx<0 then
			side =  "horiz"
		elseif ty/tx < slp then
			side =  "vert"
		else
			side =  "horiz"
		end
	elseif slp > 0 and circ_dx < 0 then
		-- moving up left
		tx = pad_x1-circ_x
		ty = pad_y1-circ_y
		if tx>=0 then
			side =  "horiz"
		elseif ty/tx < slp then
			side =  "vert"
		else
			side = "horiz"
		end
	elseif circ_dx > 0 then
		-- moving up right
		tx = pad_x0-circ_x
		ty = pad_y1-circ_y
		if tx<=0 then
			side =  "horiz"
		elseif ty/tx > slp then
			side =  "vert"
		else
			side =  "horiz"
		end
	else
		-- moving down left
		tx = pad_x1-circ_x
		ty = pad_y0-circ_y
		if tx>0 then
			side = "horiz"
		elseif ty/tx >= slp then
			side = "vert"
		else
			side = "horiz"
		end
	end
end

function draw_score(points)
	print(points)
end

function draw_bricks()
	for i=1,num_bricks do
		if brick_v[i] then
			rectfill((brick_x+brick_w)*i, 
				brick_y, 
				(((brick_x+brick_w)*i)+brick_w), 
				((brick_y)+brick_h), 
				brick_c)
		end
	end
end
